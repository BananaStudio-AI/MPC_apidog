{
  "name": "bananastudio-api-hub",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "apidog:list-projects": "node apidog/scripts/list_projects.js",
    "apidog:auth-check": "node apidog/scripts/auth_check.js",
    "apidog:list-tools": "node apidog/scripts/list_tools.js",
    "apidog:pull": "node apidog/scripts/pull_endpoints_oas.js",
    "apidog:push": "node apidog/scripts/push_endpoints.js",
    "apidog:push:oas": "node apidog/scripts/push_endpoints_oas.js",
    "apidog:push:api": "node apidog/scripts/push_oas_api.js",
    "apidog:pull:agents": "node apidog/scripts/pull_endpoints_agents.js",
    "apidog:push:agents": "node apidog/scripts/push_endpoints_agents.js",
    "apidog:validate": "node apidog/scripts/validate_specs.js",
    "apidog:import:api-hub": "tsx scripts/import_api_hub_oas.ts",
    "oas:sync": "node scripts/copy_oas_to_tracked.js",
    "generate:client": "node scripts/generate_client.js",
    "generate:api-hub-client": "OAS_FILE=./openapi/api-hub.oas.json OUTPUT_DIR=./apis/api-hub-client node scripts/generate_client.js",
    "sync:model-catalog": "tsx scripts/sync_model_catalog.ts",
    "fal:estimate": "tsx scripts/fal_pricing_estimate.ts",
    "test:client-estimate": "tsx scripts/test_client_estimate.ts",
    "sync:api-hub-client": "tsx scripts/example_sync_model_catalog.ts",
    "sync:model-registry": "tsx scripts/sync_model_registry.ts",
    "health:api-hub": "tsx scripts/healthcheck_api_hub.ts",
    "push:apidog": "tsx scripts/push_to_apidog.ts",
    "lint": "eslint . --ext .js,.mjs,.ts",
    "lint:fix": "eslint . --ext .js,.mjs,.ts --fix",
    "format": "prettier --write \"**/*.{js,mjs,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,ts,json,md}\"",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@openai/agents": "^0.3.2",
    "node-fetch": "^3.3.2",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "apidog-mcp-server": "^0.0.16",
    "eslint": "^8.57.0",
    "openapi-typescript": "^7.10.1",
    "openapi-typescript-codegen": "^0.29.0",
    "prettier": "^3.2.5",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
