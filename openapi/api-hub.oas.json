          "FAL_API"
        ],
        "servers": [
          {
            "url": "https://api.fal.ai/v1",
            "description": "FAL Platform"
          }
        ],
        "requestBody": {
          "description": "Cost estimation request with model endpoints and quantities",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/FalEstimateRequest"
              },
              "examples": {
                "singleModel": {
                  "summary": "Single model estimation",
                  "value": {
                    "estimate_type": "unit_price",
                    "endpoints": {
                      "fal-ai/flux/dev": {
                        "unit_quantity": 100
                      }
                    }
                  }
                },
                "multipleModels": {
                  "summary": "Multiple models estimation",
                  "value": {
                    "estimate_type": "unit_price",
                    "endpoints": {
                      "fal-ai/flux/dev": {
                        "unit_quantity": 50
                      },
                      "fal-ai/flux-pro": {
                        "unit_quantity": 25
                      },
                      "fal-ai/stable-diffusion-v3": {
                        "unit_quantity": 100
                      }
                    }
                  }
                }
              }
            }
          }
